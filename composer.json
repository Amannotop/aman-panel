{
    "name": "aman/panel",
    "description": "License Management Panel",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "codeigniter4/framework": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
